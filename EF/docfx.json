{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/toc.yml",
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "src": ".",
        "dest": "ef_full_dotnet_toc"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "apiPlatform": "dotnet",
      "author": "dotnet-bot",
      "ms.author": "avickers",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "searchScope": ["Entity Framework"],
      "ms.topic": "managed-reference",
      "ms.prod": "entity-framework",
      "ms.technology": "entity-framework-6",
      "feedback_system": "GitHub",
      "feedback_github_repo": "dotnet/EntityFramework.Docs",
      "feedback_product_url": "https://github.com/dotnet/ef6/issues/new"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "EntityFramework-dotnet-ref",
    "markdownEngineName": "markdig"
  }
}
